<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="geniemoviesandgames.controller.registerController">
    <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Make a new profile">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Text>
      <Pane prefHeight="278.0" prefWidth="477.0">
         <children>
            <Text layoutX="40.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text layoutX="42.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <PasswordField fx:id="firstPasswordfield" layoutX="264.0" layoutY="54.0" prefHeight="37.0" prefWidth="199.0" />
            <PasswordField fx:id="rePasswordfield" layoutX="264.0" layoutY="107.0" prefHeight="37.0" prefWidth="199.0" />
            <Text layoutX="40.0" layoutY="131.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Re-enter Password">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
              <Button fx:id="backToLoginButton" layoutX="127.0" layoutY="181.0" onAction="#switchToLogin" text="Go Back">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="finishedRegisterButton" layoutX="307.0" layoutY="181.0" mnemonicParsing="false" onAction="#registerProcedure" text="Done">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Text fx:id="warningText" layoutX="264.0" layoutY="168.0" strokeType="OUTSIDE" strokeWidth="0.0">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <TextField fx:id="userNameTextField" layoutX="264.0" layoutY="4.0" prefHeight="37.0" prefWidth="199.0" />
         </children>
      </Pane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
